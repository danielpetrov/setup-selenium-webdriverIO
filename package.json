{
  "name": "selenium-webdriverIO",
  "version": "0.0.1",
  "scripts": {
    "start-chrome": "sudo java -jar -Dwebdriver.chrome.driver='./chromedriver' selenium-server-standalone-3.0.1.jar",
    "start-mozilla": "sudo java -jar -Dwebdriver.gecko.driver='./geckodriver' selenium-server-standalone-3.0.1.jar",
    "start": "babel --copy-files --out-dir dist test.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "tape": "^4.6.3",
    "webdriverio": "^4.8.0"
  }
}
