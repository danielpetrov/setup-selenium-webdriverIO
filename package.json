{
  "name": "selenium-webdriverIO",
  "version": "0.0.1",
  "scripts": {
    "start-chrome": "java -jar -Dwebdriver.chrome.driver=node_modules/chromedriver/lib/chromedriver/chromedriver.exe selenium-server-standalone-3.0.1.jar",
    "start-ie": "java -jar -Dwebdriver.ie.driver=IEDriverServer.exe selenium-server-standalone-3.0.1.jar",
    "build": "babel --copy-files --out-dir dist test.js",
    "start": "node test.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "chromedriver": "^2.29.0",
    "geckodriver": "^1.6.1",
    "selenium-webdriver": "^3.4.0",
    "tape": "^4.6.3",
    "webdriverio": "^4.8.0"
  }
}
